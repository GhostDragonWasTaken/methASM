struct Pair {
  a: int32;
  b: int32;
}

function sum_pair(p: Pair*) -> int32 {
  return p->a + p->b;
}

function main() -> int32 {
  var p: Pair* = new Pair;
  p->a = 7;
  (*p).b = 9;

  var pa: int32* = &p->a;
  *pa = *pa + 5;

  if (pa == 0) {
    return 1;
  }

  if (p != 0) {
    return sum_pair(p);
  }

  return 0;
}
