function mark() -> int32 {
  return 1;
}

function main() -> int32 {
  var len: int32 = 11;
  var is_health: int32 = 0;

  if (len >= 11 && 'h' == 'h' && 'e' == 'e' && 'a' == 'a' && 'l' == 'l' && 't' == 't' && 'h' == 'h') {
