// Array followed by scalar â€” scalar not overwritten by array operations
function main() -> int32 {
  var arr: int32[8];
  var guard: int32 = 0x12345678;
  var i: int32 = 0;
  while (i < 8) {
    arr[i] = i * 10;
    i = i + 1;
  }
  if (guard == 0x12345678 && arr[7] == 70) {
    return 1;
  }
  return 0;
}
