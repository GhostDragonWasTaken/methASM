function max_val<T>(a: T, b: T) -> T {
    if (a > b) {
        return a;
    }
    return b;
}

function main() -> int32 {
    var a: int32 = 10;
    var b: int32 = 20;
    var result: int32 = max_val<int32>(a, b);

    var sum: int32 = 0;
    for (var i: int32 = 0; i < 3; i = i + 1) {
        sum = sum + max_val<int32>(i, 1);
    }

    return result + sum;
}
