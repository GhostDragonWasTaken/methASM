enum Direction {
    North,
    East = 2,
    South,
    West = -5
}

export function main() : int32 {
    var a : Direction = North;
    var b : Direction = East;
    var c : Direction = South;
    var d : Direction = West;

    if (a != 0) { return 1; }
    if (b != 2) { return 2; }
    if (c != 3) { return 3; }
    if (d != -5) { return 4; }

    return 0;
}
