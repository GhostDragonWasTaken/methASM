import "std/io";

function block_err() { puts(cstr("BLOCK_ERR\n")); }
function block_ok() { puts(cstr("BLOCK_OK\n")); }
function outer_err() { puts(cstr("OUTER_ERR\n")); }
function outer_ok() { puts(cstr("OUTER_OK\n")); }

function main() -> int32 {
  errdefer outer_err();
  defer outer_ok();

  {
    errdefer block_err();
    defer block_ok();
  }

  return 0;
}
